####need to read in from file; for some reason * of translation is truncated when read in from command line







from Bio.Seq import Seq
from Bio.Alphabet import IUPAC, generic_rna
#mess_rna = input("Input messenger RNA string: ")
mess_rna = open("dataset", "r").read()
mess_rna = str(mess_rna)
#print(mess_rna)
mess_rna2 = Seq(mess_rna,  IUPAC.unambiguous_rna)
mess_rna3 = Seq(mess_rna,  generic_rna)
#file = open("PROT_answer", "w")
r = mess_rna3.translate(to_stop=False)
print(r)
